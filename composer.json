{
    "name": "weboftalent\/flickr",
    "type": "silverstripe-vendormodule",
    "description": "Import,manage and render your flickr photos. Command line and GUI tools.",
    "keywords": [
        "silverstripe",
        "flickr",
        "photography"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Gordon Anderson",
            "email": "gordon.b.anderson@gmail.com",
            "homepage": "https:\/\/github.com\/gordonbanderson\/flickr-editor",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.1",
        "intervention\/image": "^2",
        "league\/climate": "^3.5",
        "matthiasmullie\/minify": "^1",
        "samwilson\/phpflickr": "^4",
        "silverstripe\/recipe-cms": "4.x-dev",
        "suilven\/freetextsearch": "dev-master",
        "suilven\/php-spriter": "dev-composerify",
        "symbiote/silverstripe-phpstan": "^4.0",
        "phpunit/phpunit": "^5.7"
    },
    "require-dev": {
        "ergebnis\/composer-normalize": "^2.7",
        "slevomat\/coding-standard": "^6.3",
        "php-parallel-lint\/php-parallel-lint": "^1.2",
        "php-parallel-lint\/php-console-highlighter": "^0.5.0",
        "phpstan\/phpstan-strict-rules": "^0.12.4",
        "phpstan\/extension-installer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Suilven\\Flickr\\": "src"
        }
    },
	"autoload-dev": {
		"psr-4": {
			"Suilven\\Flickr\\Tests\\": "tests"
		}
	},
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:gordonbanderson\/php-spriter.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https:\/\/github.com\/gordonbanderson\/flickr-editor\/issues"
    },
    "scripts": {
        "checkcs": "vendor\/bin\/phpcs --standard=ruleset.xml --extensions=php --tab-width=4 -sp src tests",
        "fixcs": "vendor\/bin\/phpcbf --standard=ruleset.xml --extensions=php --tab-width=4 -sp src tests",
        "checkCode": "composer checkcs && composer lint && composer phpstan",
        "lint": "vendor\/bin\/parallel-lint src\/ tests\/",
        "phpstan": "vendor\/bin\/phpstan analyse --memory-limit=2G --level=8 -c tests\/phpstan.neon src\/"
	}
}
